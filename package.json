{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "build": "nest build",
    "postbuild": "prisma migrate deploy",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:all": "jest --config jest-all.json --runInBand",
    "test:all:cov": "yarn test:all --coverage",
    "studio:dev": "prisma studio",
    "compodoc": "npx @compodoc/compodoc -p tsconfig.json -d ./docs",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.11.2",
    "@nestjs/bullmq": "^10.2.2",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/event-emitter": "^2.1.1",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/mapped-types": "^2.0.5",
    "@nestjs/passport": "^10.0.1",
    "@nestjs/platform-express": "^10.4.11",
    "@nestjs/platform-socket.io": "^10.4.12",
    "@nestjs/schedule": "^4.1.1",
    "@nestjs/swagger": "^7.1.8",
    "@nestjs/websockets": "^10.4.12",
    "@prisma/client": "^5.2.0",
    "axios": "^1.7.8",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.25.6",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "coolsms-node-sdk": "^2.1.0",
    "cron-parser": "^4.9.0",
    "currency.js": "^2.0.4",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "eventemitter2": "^6.4.9",
    "express": "^4.21.1",
    "firebase": "^11.0.2",
    "handlebars": "^4.7.8",
    "lodash": "^4.17.21",
    "ms": "3.0.0-canary.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.16",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "prisma-error-enum": "^0.1.3",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.1",
    "sanitize-html": "^2.13.1",
    "socket.io": "^4.8.1",
    "swagger-ui-express": "^5.0.0",
    "tslib": "^2.3.0",
    "twilio": "^5.3.6",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.21",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/babel__core": "^7.20.5",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.4",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.14.199",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.3.1",
    "@types/nodemailer": "^6.4.11",
    "@types/passport-jwt": "^3.0.9",
    "@types/supertest": "^2.0.12",
    "@types/twilio": "^3.19.3",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "prisma": "^5.2.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1"
    }
  }
}
